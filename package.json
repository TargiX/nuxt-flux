{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "dev:local": "cp .env.local .env && nuxt dev",
    "dev:prod": "cp .env.production .env && nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare && prisma generate",
    "lint": "eslint .",
    "lint:fix": "eslint --fix --ext .js,.ts,.vue .",
    "format": "prettier --write \"**/*.{js,ts,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "db:studio:local": "cp .env.local .env && npx prisma studio",
    "db:studio:prod": "cp .env.production .env && npx prisma studio",
    "db:migrate:local": "cp .env.local .env && npx prisma migrate deploy",
    "db:migrate:prod": "cp .env.production .env && npx prisma migrate deploy",
    "test": "vitest"
  },
